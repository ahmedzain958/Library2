<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".designpatterns.linkedin.LikedInFoundationsDesignPatternsActivity">

        <Button
            android:id="@+id/buttonBasicCameraApp"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="Basic Camera App"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonCameraPlusApp"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:text="Camera plus App"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/buttonBasicCameraApp"
            app:layout_constraintStart_toStartOf="@+id/buttonBasicCameraApp"
            app:layout_constraintTop_toBottomOf="@+id/buttonBasicCameraApp" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>